"delimitMate config

let delimitMate_expand_cr = 2
let delimitMate_expand_space = 1
let delimitMate_matchpairs = "(:),[:],{:},<:>"

"au FileType *.cpp let b:delimitMate_expand_cr = 1
"au FileType *.c let b:delimitMate_expand_cr = 1
"au FileType *.h let b:delimitMate_expand_cr = 1
"au FileType *.hpp let b:delimitMate_expand_cr = 1
"au FileType *.cs let b:delimitMate_expand_cr = 1
"au FileType *.java let b:delimitMate_expand_cr = 1
