application/ms-excel; libreoffice --calc %U %s;
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; libreoffice --calc %U %s;

application/ppt; libreoffice --impress %U %s;
application/vnd.ms-powerpoint; libreoffice --impress %U %s;
application/x-mspowerpoint; libreoffice --impress %U %s;

application/pdf; mutt_bgrun acroread -tempFile -openInNewInstance %s;

image/*; mutt_bgrun ristretto %s;

